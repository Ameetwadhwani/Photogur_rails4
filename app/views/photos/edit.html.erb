<h2>Edit a Photo</h2>
<%= link_to "Back to Pictures", photos_url %>


<%= form_for @photo do |f| %>
    <p>
      <%= label_tag 'Artist' %>
      <%= f.text_field :artist  %>

    </p>
    <p>
      <%= label_tag 'Title' %>
      <%= f.text_field :title  %>
    </p>
    <p>
      <%= label_tag 'URL' %>
      <%= f.text_field :url  %>
    </p>
    <p> <%= submit_tag 'Save' %> </p>

<% end %>